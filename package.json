{
  "name": "blog",
  "version": "0.3.0",
  "description": "Yet another blog",
  "private": true,
  "engines": {
    "node": ">=18.12.1 <19",
    "npm": ">=9 <10"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "node dist/index.js",
    "build:dev": "node scripts/build-dev.mjs",
    "build": "del ./dist && cross-env NODE_ENV=production node scripts/build-prod.mjs",
    "watch:dev": "node scripts/watch-dev.mjs",
    "watch:node": "nodemon --watch dist dist/index.js | pino-pretty -S",
    "watch": "concurrently -n \"BUILD,NODE\" -c \"blue,green\" \"npm:watch:dev\" \"npm:watch:node\"",
    "prepare": "husky install",
    "db:migrate": "node scripts/migrate.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git@gitstash.me:blog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "Igor <dont@bzzr.me> (https://ignorethis.page/)",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@tinyhttp/app": "2.0.22",
    "@tinyhttp/logger": "1.3.0",
    "@types/jest": "28.1.6",
    "@types/markdown-it": "12.2.3",
    "@types/node": "16.11.46",
    "@types/sqlite3": "3.1.8",
    "blog-css": "0.3.2",
    "chalk": "5.0.1",
    "concurrently": "7.3.0",
    "cross-env": "7.0.3",
    "dayjs": "1.11.6",
    "del-cli": "5.0.0",
    "esbuild": "0.14.51",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "markdown-it": "13.0.1",
    "nodemon": "2.0.19",
    "pino-http": "8.2.0",
    "pino-pretty": "9.1.0",
    "prettier": "2.7.1",
    "sirv": "2.0.2",
    "sqlite": "4.1.2",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "shiki": "0.10.1",
    "sqlite3": "5.0.11"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
