{
  "name": "blog",
  "version": "0.2.1",
  "description": "Yet another blog",
  "private": true,
  "engines": {
    "node": ">=16.16.0 <17",
    "npm": ">=8.11.0 <9"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "node dist/index.js",
    "build:dev": "node scripts/build-dev.mjs",
    "build": "cross-env NODE_ENV=production node scripts/build-prod.mjs",
    "watch:dev": "node scripts/watch-dev.mjs",
    "watch:node": "nodemon --watch dist dist/index.js",
    "watch": "concurrently -n \"TS,NODE\" -c \"blue,green\" \"npm:watch:dev\" \"npm:watch:node\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@gitstash.me:blog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "Igor <dont@bzzr.me> (https://ignorethis.page/)",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@tinyhttp/app": "2.0.22",
    "@tinyhttp/logger": "1.3.0",
    "@types/jest": "28.1.6",
    "@types/markdown-it": "12.2.3",
    "@types/node": "16.11.46",
    "blog-css": "0.2.12",
    "concurrently": "7.3.0",
    "cross-env": "7.0.3",
    "esbuild": "0.14.51",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "markdown-it": "13.0.1",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "sirv": "2.0.2",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "shiki": "0.10.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
